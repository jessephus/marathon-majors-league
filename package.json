{
  "name": "marathon-majors-fantasy-league",
  "version": "2.0.0",
  "description": "Marathon Majors Fantasy League - Daily fantasy sports for major marathons",
  "type": "module",
  "main": "pages/index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "deploy": "vercel --prod",
    "postbuild": "node scripts/init-db.js",
    "seed": "node scripts/init-db.js",
    "cleanup:test-races": "node scripts/cleanup-test-races.js",
    "test": "node tests/run-tests.js",
    "test:api": "node tests/api-endpoints.test.js",
    "test:db": "node tests/database.test.js",
    "test:frontend": "node tests/frontend-integration.test.js",
    "test:flow": "node tests/game-flow.test.js",
    "test:nextjs": "node tests/nextjs-routing.test.js",
    "test:salarycap": "node tests/salary-cap-draft.test.js",
    "test:commissioner": "node tests/commissioner-panels.test.js",
    "test:race": "node tests/race-management.test.js",
    "test:performance": "node tests/performance-benchmarks.test.js",
    "test:legacy": "node tests/legacy-regression.test.js",
    "test:quality": "node scripts/validate-test-quality.js",
    "test:verify": "node scripts/verify-tests-catch-bugs.js",
    "test:state": "tsx tests/state-manager.test.js",
    "test:state:integration": "tsx tests/state-manager-integration.test.js",
    "test:draft": "node src/features/draft/__tests__/validation.test.js",
    "test:api-client": "tsx tests/api-client.test.js",
    "test:ssr": "node tests/ssr-integration.test.js",
    "test:ssr:landing": "node tests/landing-page-ssr.test.js",
    "test:ssr:team": "node tests/team-session-ssr.test.js",
    "test:formatting": "node tests/formatting-utils.test.js",
    "test:dynamic": "tsx tests/dynamic-imports.test.js",
    "test:dynamic:e2e": "node tests/dynamic-import-e2e.test.js",
    "test:perf-instrumentation": "tsx tests/performance-instrumentation.test.js",
    "test:coverage": "c8 --reporter=text --reporter=html --reporter=json-summary npm test",
    "test:api-client:coverage": "tsx tests/api-client-coverage.test.js",
    "test:web-vitals:coverage": "tsx tests/web-vitals-coverage.test.js",
    "test:session-manager:coverage": "tsx tests/session-manager-coverage.test.js",
    "test:feature-flags:coverage": "tsx tests/feature-flags-coverage.test.js",
    "test:dynamic-import:coverage": "tsx tests/dynamic-import-coverage.test.js",
    "test:coverage:new": "c8 --reporter=text --reporter=json-summary --include='lib/**/*.ts' --include='lib/**/*.tsx' --include='components/**/*.tsx' --lines 70 --functions 60 --branches 70 -- sh -c 'npm run test:state && npm run test:dynamic && npm run test:perf-instrumentation && npm run test:api-client:coverage && npm run test:dynamic-import:coverage && npm run test:feature-flags:coverage && npm run test:session-manager:coverage && npm run test:web-vitals:coverage && npm run test:ssr'",
    "validate:no-legacy": "node scripts/validate-no-legacy-imports.js"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.4.2",
    "@chakra-ui/react": "^3.29.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^1.0.2",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "next": "^15.5.6",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "speakeasy": "^2.0.0",
    "swr": "^2.3.6",
    "web-vitals": "^5.1.0"
  },
  "keywords": [
    "fantasy",
    "marathon",
    "running",
    "game"
  ],
  "author": "Jesse Geraci",
  "license": "MIT",
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.1",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "c8": "^10.1.3",
    "sharp": "^0.34.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
